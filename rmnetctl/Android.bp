//

cc_binary {

    name: "rmnetcli",
    vendor: true,
    srcs: ["rmnetcli.c"],
    cflags: ["-Wall","-Werror",],
    header_libs: ["rmnetctl_headers"],
    shared_libs: ["librmnetctl"],

}

cc_library_shared {

    name: "librmnetctl",
    vendor: true,
    srcs: ["librmnetctl.c"],
    cflags: ["-Wall","-Werror"],
    local_include_dirs: ["."],
    header_libs: ["generated_kernel_headers","rmnetctl_headers"],

}

cc_library_headers {
    name: "rmnetctl_headers",
    vendor: true,
    export_include_dirs: ["."],
}


